{
    "name": "kingscode/laravel-localize",
    "description": "Laravel Localization in a yiffy.",
    "license": "MIT",
    "authors": [
        {
            "name": "Koen Hoeijmakers",
            "email": "koen@hoeijmakers.me"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.9",
        "phpunit/phpunit": "^9.3.3",
        "mockery/mockery": "^1.4.2"
    },
    "autoload": {
        "psr-4": {
            "KingsCode\\LaravelLocalize\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KingsCode\\LaravelLocalize\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KingsCode\\LaravelLocalize\\LocalizeServiceProvider"
            ]
        }
    }
}
