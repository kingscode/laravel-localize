{
    "name": "koenhoeijmakers/laravel-localize",
    "description": "Laravel Localization in a yiffy.",
    "license": "MIT",
    "authors": [
        {
            "name": "Koen Hoeijmakers",
            "email": "koen@hoeijmakers.me"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "laravel/framework": "^5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.7",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "KoenHoeijmakers\\LaravelLocalize\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KoenHoeijmakers\\LaravelLocalize\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KoenHoeijmakers\\LaravelLocalize\\LocalizeServiceProvider"
            ]
        }
    }
}
